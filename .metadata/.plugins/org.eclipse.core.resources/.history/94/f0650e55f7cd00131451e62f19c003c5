package fractalsWeitzman;

/**
 * @author yael
 *
 */
public class Multiplying extends Action {
	public Complex perform(Complex c1,Complex c2) {
		return c1.times(c2);//the plus method returns new complex obj, so the original params didnt changed
		
	}
	
	public Complex perform(Complex c1,double d) {
		return c1.times(d);//the plus method returns new complex obj, so the original params didnt changed
		
	}
	
	public Complex perform(double d,Complex c1) {
		return c1.times(d);//the plus method returns new complex obj, so the original params didnt changed
		
	}

}
